<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ParkingStatusController">
    <children>
        <Label layoutX="41.0" layoutY="31.0" text="Available Spots :" textAlignment="CENTER">
            <font>
                <Font size="15.0" />
            </font>
        </Label>
        <Label fx:id="totalSpotsLabel" layoutX="155.0" layoutY="26.0">
            <font>
                <Font size="20.0" />
            </font>
        </Label>
        <TableView fx:id="statusTable" layoutX="347.0" layoutY="79.0" prefHeight="257.0" prefWidth="208.0">
            <columns>
                <TableColumn fx:id="statusDate" prefWidth="75.0" text="Date" />
                <TableColumn fx:id="statusSpots" prefWidth="75.0" text="Spots" />
            </columns>
        </TableView>
        <Button fx:id="statusBtn" layoutX="347.0" layoutY="29.0" mnemonicParsing="false" onAction="#showAllStatues" prefHeight="26.0" prefWidth="101.0" text="Show Status" />
        <PieChart fx:id="pieChart" layoutX="37.0" layoutY="80.0" prefHeight="258.0" prefWidth="287.0" />
    </children>
