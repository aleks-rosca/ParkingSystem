    <children>
        <TabPane fx:id="MCClient" layoutY="-1.0" prefHeight="400.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE" >
            <tabs>
                <Tab text="Parking Status">
                    <fx:include source="../View/Parking Status.fxml"/>
                </Tab>
                <Tab text="Manage Employee">
                    <fx:include source="../View/Manage Employee.fxml"/>
                </Tab>
                <Tab text="Employee Reservation">
                    <fx:include source="../View/Employee Reservation.fxml"/>
                </Tab>
                <Tab text="Guest Reservation">
                    <fx:include source="../View/Guest Reservation.fxml"/>
                </Tab>
            </tabs>
        </TabPane>

    </children>
</AnchorPane>
